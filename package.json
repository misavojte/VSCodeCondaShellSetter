{
    "name": "windows-conda-powershell-setup",
    "displayName": "Windows Conda PowerShell Setup",
    "description": "Automatically configures Conda in Windows PowerShell terminals for VS Code.",
    "version": "0.0.1",
    "icon": "icon.png",
    "publisher": "vojtechovska",
    "engines": {
      "vscode": ">=1.70.0"
    },
    "categories": [
      "Other"
    ],
    "main": "./dist/extension.js",
    "contributes": {
      "commands": [
        {
          "command": "condaTerminalSetup.applySettings",
          "title": "Initialize Conda in PowerShell",
          "category": "Conda Setup"
        },
        {
          "command": "condaTerminalSetup.resetSettings",
          "title": "Remove Conda from PowerShell",
          "category": "Conda Setup"
        }
      ]
    },
    "scripts": {
      "vscode:prepublish": "tsc -p ./",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "package": "vsce package",
      "publish": "vsce publish"
    },
    "devDependencies": {
      "@types/node": "^18.0.0",
      "@types/vscode": "^1.70.0",
      "typescript": "^4.7.4",
      "@vscode/vsce": "^2.24.0"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/your-username/conda-terminal-setup.git"
    },
    "license": "MIT",
    "keywords": [
      "conda",
      "windows",
      "powershell",
      "miniconda",
      "vscode-extension"
    ]
  }