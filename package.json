{
    "name": "conda-terminal-setup",
    "displayName": "Conda Terminal Setup",
    "description": "Automatically adds Conda to the VS Code PowerShell terminal with a user-prompted installation path.",
    "version": "0.0.1",
    "publisher": "your-publisher-name",
    "engines": {
      "vscode": ">=1.70.0"
    },
    "categories": [
      "Other"
    ],
    "main": "./dist/extension.js",
    "contributes": {
      "commands": [
        {
          "command": "condaTerminalSetup.applySettings",
          "title": "Apply Conda Terminal Settings",
          "category": "Conda Setup"
        },
        {
          "command": "condaTerminalSetup.resetSettings",
          "title": "Reset Conda Terminal Settings",
          "category": "Conda Setup"
        }
      ]
    },
    "scripts": {
      "vscode:prepublish": "tsc -p ./",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "package": "vsce package",
      "publish": "vsce publish"
    },
    "devDependencies": {
      "@types/node": "^18.0.0",
      "@types/vscode": "^1.70.0",
      "typescript": "^4.7.4",
      "@vscode/vsce": "^2.24.0"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/your-username/conda-terminal-setup.git"
    },
    "license": "MIT",
    "keywords": [
      "conda",
      "terminal",
      "powershell",
      "vscode-extension",
      "environment"
    ]
  }